{
  "name": "n8n-nodes-aparavi",
  "version": "1.0.5",
  "description": "n8n community nodes for Aparavi DTC API. Run any workflow including PII/HIPAA anonymization, OCR, document parsing, audio transcription, and custom pipelines.",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "n8n-node",
    "n8n-community",
    "pii-censor",
    "pii-anonymization",
    "hipaa-censor",
    "hipaa-anonymization",
    "pii-hipaa",
    "anonymize",
    "anonymization",
    "censor",
    "censoring",
    "privacy",
    "data-protection",
    "aparavi",
    "aparavi-dtc",
    "gdpr",
    "hipaa",
    "healthcare-data",
    "personally-identifiable-information",
    "data-privacy",
    "workflow",
    "automation",
    "ocr",
    "document-parsing",
    "audio-transcription",
    "custom-pipeline"
  ],
  "license": "MIT",
  "homepage": "https://github.com/AparaviSoftware/aparavi-n8n-node",
  "author": {
    "name": "armin Foroughi",
    "email": "armin.foroughi@aparavi.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AparaviSoftware/aparavi-n8n-node.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build:icons && gulp copy:pipelines",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint . --ext .js,.ts --ignore-path .gitignore",
    "lintfix": "eslint . --ext .js,.ts --ignore-path .gitignore --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "predefinedPipelines"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AparaviApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/AparaviUnified/AparaviUnified.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^16.18.68",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.42.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "aparavi-client": "^1.0.6",
    "aparavi-dtc-node-sdk": "file:../aparavi-dtc-node-sdk"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
